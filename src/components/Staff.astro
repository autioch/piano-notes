---
type Props = {
  width: number;
};

const { width } = Astro.props;
---

<rect x="0" y="1" width={width} height="0.5" fill="#000"></rect>
<rect x="0" y="7" width={width} height="0.5" fill="#000"></rect>
<rect x="0" y="13" width={width} height="0.5" fill="#000"></rect>
<rect x="0" y="19" width={width} height="0.5" fill="#000"></rect>
<rect x="0" y="25" width={width} height="0.5" fill="#000"></rect>
